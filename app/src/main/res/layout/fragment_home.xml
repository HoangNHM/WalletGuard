<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="hoangnhm.walletguard.Screen.HomeFragment">

    <!-- TODO: Update blank fragment layout -->
    <ImageButton
        android:id="@+id/mBtnList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@android:color/transparent"
        android:src="@android:drawable/btn_star"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/mBtnInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/mBtnList"
        android:layout_toStartOf="@id/mBtnList"
        android:layout_toLeftOf="@id/mBtnList"
        android:background="@android:color/transparent"
        android:src="@android:drawable/btn_star"
        tools:ignore="ContentDescription" />

</RelativeLayout>
